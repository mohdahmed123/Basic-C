#include <stdio.h>
void swap(int a,int b)
{int c;
    c=a;
    a=b;
    b=c;
    printf("***********************\n");
    printf("AFTER SWAPING\n");
    printf("a is %d\n",a);
    printf("b is %d\n",b);
    printf("******CODE IS END******\n");
}
void main()
{   int a,b;
    printf("*****CODE IS START*****\n");
    printf("enter your number a ");
    scanf("%d",&a);
    printf("enter your number b ");
    scanf("%d",&b);
    swap(a,b);
}

